  object qrySubTotal: TZReadOnlyQuery
    Connection = DM.conn
    SQL.Strings = (
      
        'select sum((qty_karton*hrg_beli_karton)+((qty_biji/biji_per_kart' +
        'on)*hrg_beli_karton)) as sub_total '
      'from transaksi.nota_detail'
      'where id_nota=:pid_nota')
    Params = <
      item
        DataType = ftUnknown
        Name = 'pid_nota'
        ParamType = ptUnknown
      end>
    Left = 636
    Top = 10
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'pid_nota'
        ParamType = ptUnknown
      end>
    object qrySubTotalsub_total: TFloatField
      FieldName = 'sub_total'
