  object qBrowse: TZQuery
    Connection = DM.conn
    BeforeOpen = qBrowseBeforeOpen
    CachedUpdates = True
    SQL.Strings = (
      
        'SELECT a.id_purchase_receive, a.no_purchase_receive, a.dt_receiv' +
        'e, a.usr_upd, a.ispost, a.iscancel, a.kd_rekanan, b.nama_rekanan' +
        ', a.reff_number'
      'FROM  purchasing.purchase_receive a'
      'LEFT JOIN master.mrekanan b ON b.kd_rekanan=a.kd_rekanan'
      'WHERE a.isdelete='#39'0'#39' and a.id_trans=:pid_trans'
      
        'and a.dt_receive between to_timestamp(:ptgl0,'#39'dd/mm/yyyy'#39') and t' +
        'o_timestamp(:ptgl1,'#39'dd/mm/yyyy'#39') +  interval '#39'23 hours 59 min'#39)
    Params = <
