        DataType = ftString
        Name = 'pid_trans'
        ParamType = ptUnknown
        Value = '102'
      end
      item
        DataType = ftString
        Name = 'ptgl0'
        ParamType = ptUnknown
        Value = '01/01/2013'
      end
      item
        DataType = ftString
        Name = 'ptgl1'
        ParamType = ptUnknown
        Value = '01/01/2015'
      end>
    Left = 51
    Top = 44
