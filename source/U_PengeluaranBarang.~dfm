    end
    object qryPOdt_nota: TDateTimeField
      DisplayLabel = 'Tgl. Faktur'
      DisplayWidth = 18
      FieldName = 'dt_nota'
      Required = True
    end
    object qryPOno_bukti: TStringField
      DisplayWidth = 20
      FieldName = 'no_bukti'
      Size = 120
    end
    object qryPOnama_supplier: TStringField
      DisplayWidth = 50
      FieldName = 'nama_supplier'
      Required = True
      Size = 1020
    end
    object qryPOjns_transaksi: TStringField
      DisplayLabel = 'Nama Supplier'
      DisplayWidth = 50
      FieldName = 'jns_transaksi'
      Visible = False
      Size = 400
    end
    object qryPOkd_rekanan: TStringField
      FieldName = 'kd_rekanan'
      Required = True
      Visible = False
      Size = 80
    end
  end
  object qryupdate: TZQuery
    Connection = DM.conn
    AfterOpen = DetailAfterOpen
    OnCalcFields = DetailCalcFields
    CachedUpdates = True
    BeforeInsert = DetailBeforeInsert
    BeforeEdit = DetailBeforeEdit
    BeforePost = DetailBeforePost
    AfterPost = DetailAfterPost
    OnNewRecord = DetailNewRecord
    Params = <>
    Left = 448
    Top = 69
  end
end
