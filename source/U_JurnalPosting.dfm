  object qPayment: TZQuery
    Connection = DM.conn
    CachedUpdates = True
    SQL.Strings = (
      'select '
      'a.no_payment,'
      'a.id_payment,'
      'a.dt_payment,'
      'a.kd_rekanan,'
      'b.nama_rekanan,'
      'a.id_trans,'
      'a.nominal,'
      'a.ispost,'
      'a.iscancel,'
      'a.usr_ins,'
      'a.dt_ins,'
      'a.usr_upd,'
      'a.id_rek_gl,'
      'a.usr_post,'
      'a.dt_post,'
      'a.no_cek_bg_tt,'
      'a.jns_ttbg,'
      'r.descriptions as nama_rekening'
      'from accfin.payment a'
      'left join master.mrekanan b on b.kd_rekanan=a.kd_rekanan'
      'left join master.mrek_gl r ON r.id_rek_gl=a.id_rek_gl'
      'where a.isdelete='#39'0'#39' and a.id_trans=:pid_trans'
      
        'and a.dt_payment between to_timestamp(:ptgl0,'#39'dd/mm/yyyy'#39') and t' +
        'o_timestamp(:ptgl1,'#39'dd/mm/yyyy'#39') +  interval '#39'23 hours 59 min'#39
      'order by a.id_payment')
    Params = <
      item
        DataType = ftString
        Name = 'pid_trans'
        ParamType = ptUnknown
        Value = '401'
      end
      item
        DataType = ftString
