  object qBrowse: TZReadOnlyQuery
    Connection = DM.conn
    BeforeOpen = qBrowseBeforeOpen
    SQL.Strings = (
      
        'select a.id_payment, a.no_payment, a.dt_payment, a.id_rek_gl, a.' +
        'usr_upd, a.ispost, a.iscancel, b.nama_rekanan, a.usr_post, a.dt_' +
        'post, a.nominal'
      'from accfin.payment a'
      'left join master.mrekanan b on b.kd_rekanan=a.kd_rekanan'
      'where a.isdelete='#39'0'#39' and a.id_trans=:pid_trans'
      
        'and a.dt_payment between to_timestamp(:ptgl0,'#39'dd/mm/yyyy'#39') and t' +
        'o_timestamp(:ptgl1,'#39'dd/mm/yyyy'#39') +  interval '#39'23 hours 59 min'#39' ')
    Params = <
      item
        DataType = ftString
        Name = 'pid_trans'
        ParamType = ptUnknown
        Value = '403'
      end
      item
        DataType = ftString
        Name = 'ptgl0'
        ParamType = ptUnknown
        Value = '01/01/2012'
      end
      item
        DataType = ftString
        Name = 'ptgl1'
        ParamType = ptUnknown
        Value = '01/01/2015'
      end>
    Left = 257
