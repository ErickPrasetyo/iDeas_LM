      'select a.kd_item, a.diskripsi, a.hrg, b.dt_nota, c.nama_rekanan'
      'from transaksi.nota_detail a'
      'join transaksi.nota b on b.id_nota=a.id_nota'
      'join master.mrekanan c on c.kd_rekanan=b.kd_rekanan'
