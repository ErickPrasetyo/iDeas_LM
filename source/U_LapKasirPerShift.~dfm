  object qTotalUser: TZQuery
    Connection = DM.conn
    SQL.Strings = (
      'select '
      
        '(case when c.vuser is null then '#39'Administrator'#39' else c.full_name' +
        ' end) as nama_kasir,'
      'sum(a.total) as total,'
      'b.dt_transaksi, b.an as kepala_kasir, b.shift'
      'from accfin.kasir_detail a'
      'left join accfin.kasir b on b.id=a.id_kasir'
      'left join master.vuser c on c.vuser=a.nama_kasir'
      'where a.id_kasir=:id_kasir'
      'group by c.vuser, c.full_name, b.dt_transaksi, b.an, b.shift'
      'order by c.full_name asc')
    Params = <
      item
        DataType = ftUnknown
        Name = 'id_kasir'
        ParamType = ptUnknown
      end>
    Left = 716
    Top = 72
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'id_kasir'
        ParamType = ptUnknown
      end>
    object qTotalUsernama_kasir: TStringField
      FieldName = 'nama_kasir'
      Size = 120
    end
    object qTotalUsertotal: TFloatField
      FieldName = 'total'
      ReadOnly = True
    end
    object qTotalUserdt_transaksi: TDateTimeField
      FieldName = 'dt_transaksi'
    end
    object qTotalUserkepala_kasir: TStringField
      FieldName = 'kepala_kasir'
      Size = 400
    end
    object qTotalUsershift: TIntegerField
      FieldName = 'shift'
      Required = True
    end
  end
  object frxDBTotal: TfrxDBDataset
    UserName = 'frxDBTotal'
    CloseDataSource = False
    FieldAliases.Strings = (
      'nama_kasir=nama_kasir'
      'total=total'
      'dt_transaksi=dt_transaksi'
      'kepala_kasir=kepala_kasir'
      'shift=shift')
    DataSet = qTotalUser
    BCDToCurrency = False
    Left = 714
    Top = 106
  end
