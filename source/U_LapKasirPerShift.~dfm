  object Master: TZQuery
    Connection = DM.conn
    BeforeOpen = MasterBeforeOpen
    CachedUpdates = True
    BeforeEdit = MasterBeforeEdit
    BeforePost = MasterBeforePost
    BeforeDelete = MasterBeforeDelete
    OnNewRecord = MasterNewRecord
    SQL.Strings = (
      'select * '
      'from accfin.kasir'
      'where id_trans=:pid_trans and isdelete='#39'0'#39' and'
      
        'dt_kasir between to_timestamp(:ptgl0,'#39'dd/mm/yyyy'#39') and to_timest' +
        'amp(:ptgl1,'#39'dd/mm/yyyy'#39') +  interval '#39'23 hours 59 min'#39
      'order by dt_kasir, shift')
    Params = <
      item
        DataType = ftUnknown
        Name = 'pid_trans'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'ptgl0'
        ParamType = ptUnknown
        Value = '01/01/2009'
      end
      item
        DataType = ftUnknown
        Name = 'ptgl1'
        ParamType = ptUnknown
      end>
    Left = 444
