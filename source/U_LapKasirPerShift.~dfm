      'select a.*, b.nama_rekanan'
      'from transaksi.nota a'
      'left join master.mrekanan b on b.kd_rekanan=a.kd_rekanan'
      
        'where a.id_trans='#39'201'#39' and a.ispost='#39'1'#39' and a.isdelete='#39'0'#39' and a' +
