  object qBrowse: TZQuery
    Connection = DM.conn
    BeforeOpen = qBrowseBeforeOpen
    CachedUpdates = True
    SQL.Strings = (
      
        'SELECT a.id_mutasi, a.mutasi_no, a.dt_mutasi, a.usr_upd, a.ispos' +
        't, a.iscancel, a.description'
      'FROM  inventory.mutasi a'
      'WHERE a.isdelete='#39'0'#39' and a.id_trans=:pid_trans'
      
        'and a.dt_mutasi between to_timestamp(:ptgl0,'#39'dd/mm/yyyy'#39') and to' +
        '_timestamp(:ptgl1,'#39'dd/mm/yyyy'#39') +  interval '#39'23 hours 59 min'#39
      'order by a.id_mutasi')
    Params = <
      item
        DataType = ftString
        Name = 'pid_trans'
        ParamType = ptUnknown
        Value = '102'
      end
      item
        DataType = ftString
