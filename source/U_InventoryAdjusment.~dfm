  object LItem: TZReadOnlyQuery
    Connection = DM.conn
    SQL.Strings = (
      'select '
      'c.nama_item, '
      'c.kd_item, '
      'c.kd_satuan, '
      'a.id_warehouse, '
      'c.id_cat_item, '
      'd.description,'
      
        'coalesce(sum(coalesce(a.qty_in,0)-coalesce(a.qty_ot,0)),0) as st' +
        'ok'
      ''
      'from master.item c'
      'left join inventory.stock a on a.id_item=c.kd_item'
      'left join master.mcat_item d on d.id_cat_item=c.id_cat_item'
      ''
      'where c.isdetail='#39'1'#39
      
        'group by c.nama_item, c.kd_item, c.kd_satuan, a.id_warehouse, c.' +
        'id_cat_item, d.description'
      'order by (c.id_cat_item, c.nama_item)')
    Params = <>
    Left = 548
    Top = 11
    object LItemnama_item: TStringField
      DisplayLabel = 'DISKRIPSI'
      DisplayWidth = 50
      FieldName = 'nama_item'
      Size = 255
    end
    object LItemkd_item: TStringField
      DisplayLabel = 'KODE'
      DisplayWidth = 15
      FieldName = 'kd_item'
      Required = True
      Size = 50
    end
    object LItemid_warehouse: TStringField
      DisplayLabel = 'LOKASI'
      DisplayWidth = 12
      FieldName = 'id_warehouse'
      Required = True
      Size = 12
    end
    object LItemstok: TFloatField
      DisplayLabel = 'STOK'
      DisplayWidth = 10
      FieldName = 'stok'
      ReadOnly = True
    end
    object LItemkd_satuan: TStringField
      DisplayLabel = 'SATUAN'
      DisplayWidth = 12
      FieldName = 'kd_satuan'
      Size = 12
    end
    object LItemdescription: TStringField
      DisplayLabel = 'JENIS'
      DisplayWidth = 25
      FieldName = 'description'
      Size = 400
    end
  end
